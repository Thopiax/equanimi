{
  "name": "equanimi",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.7.0",
  "scripts": {
    "dev:browser": "pnpm --filter @equanimi/browser dev",
    "dev:desktop": "pnpm --filter @equanimi/desktop dev",
    "build:browser": "pnpm --filter @equanimi/browser build",
    "build:desktop": "pnpm --filter @equanimi/desktop build",
    "build": "pnpm -r build",
    "typecheck": "pnpm -r typecheck",
    "changeset": "changeset",
    "version-packages": "changeset version && node scripts/sync-versions.js",
    "release": "changeset publish"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.2",
    "@changesets/cli": "^2.29.8"
  }
}
